{
  "name": "sprint3-1",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test:prepare": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test prisma db push && prisma generate",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test node -r dotenv/config ./node_modules/jest/bin/jest.js --config jest.config.ts --runInBand",
    "test:cov": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test node -r dotenv/config ./node_modules/jest/bin/jest.js --config jest.config.ts --runInBand --coverage",
    "test:watch": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test node -r dotenv/config ./node_modules/jest/bin/jest.js --config jest.config.ts --runInBand --watch",
    "seed": "ts-node prisma/seed.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
